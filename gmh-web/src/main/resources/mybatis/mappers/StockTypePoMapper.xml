<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zes.squad.gmh.mapper.StockTypeMapper">
	<resultMap id="BaseResultMap" type="com.zes.squad.gmh.entity.po.StockTypePo">
		<id column="id" jdbcType="BIGINT" property="id" />
		<result column="name" jdbcType="VARCHAR" property="name" />
		<result column="store_id" jdbcType="BIGINT" property="storeId" />
		<result column="created_time" jdbcType="TIMESTAMP" property="createdTime" />
		<result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
	</resultMap>
	<sql id="BaseColumnList">
		`id`, `name`, `store_id`,
		`created_time`, `modified_time`
	</sql>
	<insert id="insert" parameterType="com.zes.squad.gmh.entity.po.StockTypePo">
		INSERT INTO stock_type
		(`name`, `store_id`)
		VALUES
		(#{name, jdbcType=VARCHAR})
	</insert>
	<delete id="deleteById" parameterType="long">
		DELETE FROM stock_type WHERE id = #{id, jdbcType=BIGINT}
	</delete>
	<delete id="batchDelete" parameterType="list">
		DELETE FROM stock_type WHERE id IN
		<foreach collection="list" item="id" separator="," open="(" close=")">
			#{id, jdbcType=BIGINT}
		</foreach>
	</delete>
	<update id="updateSelective" parameterType="com.zes.squad.gmh.entity.po.StockTypePo">
		UPDATE store
		<set>
			<if test="name != null and name != ''">
				`name = `#{name, jdbcType=VARCHAR}
			</if>
		</set>
		WHERE `id` = #{id, jdbcType=BIGINT}
	</update>
</mapper>